
[phases.setup]
nixPkgs = ["python39", "python39Packages.pip", "python39Packages.setuptools", "python39Packages.wheel"]

[phases.install]
cmds = ["cd faq-backend && python -m ensurepip --user", "cd faq-backend && python -m pip install --user --upgrade pip setuptools wheel", "cd faq-backend && python -m pip install --user -r requirements-railway.txt"]

[start]
cmd = "cd faq-backend && python railway_start.py"

[variables]
PYTHONPATH = "faq-backend"
